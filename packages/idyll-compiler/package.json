{
  "name": "idyll-compiler",
  "version": "5.0.0-alpha.0",
  "description": "Compiler for idyll",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src -d dist/cjs",
    "build:es": "cross-env BABEL_ENV=es babel src -d dist/es",
    "compile": "nearleyc src/grammar.ne -o src/grammar.js",
    "build": "npm run compile && npm run build:es && npm run build:cjs",
    "dev": "concurrently \"yarn run compile\" \"yarn run build:es --watch\" \"yarn run build:cjs --watch\"",
    "test": "npm run compile && mocha",
    "nearley-debug": "nearley-test src/grammar.js -i 'OPEN_BRACKET COMPONENT_NAME TOKEN_VALUE_START \"Slideshow\" TOKEN_VALUE_END CLOSE_BRACKET OPEN_BRACKET FORWARD_SLASH COMPONENT_NAME TOKEN_VALUE_START \"Slideshow\" TOKEN_VALUE_END CLOSE_BRACKET EOF'",
    "prepublish": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idyll-lang/idyll.git"
  },
  "keywords": [
    "idyll",
    "compiler"
  ],
  "author": "Matthew Conlen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idyll-lang/idyll/issues"
  },
  "homepage": "https://github.com/idyll-lang/idyll#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.0.5",
    "expect.js": "^0.3.1",
    "mocha": "6.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "gray-matter": "^3.1.1",
    "idyll-ast": "^2.2.6",
    "lex": "^1.7.9",
    "nearley": "^2.20.1",
    "smartquotes": "^2.0.0"
  }
}
